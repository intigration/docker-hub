$map(
$.**.repositories,	   
function($v, $i, $a) {	       
    $merge([
        {
            'type': 1,
            'description':'container image for ' & $v,
		"logo": "https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/bitwarden.png",
            'name':$v,
            'tags': $a[$i].tags,
            'properties': $a[$i].properties,
            'title': 'M3_Edge_' & $v,
            'platform': $a[$i].platform,
            'image': "registry.m3-solutions.io:8443/" & $v,
             "categories": [
      $a[$i].categories
    ],
     "restart_policy":       "always"
,
    "ports": [
        $a[$i].ports

    ],
    "volumes": [
        {
				"container": "/var/run/docker.sock",
				"bind": "/var/run/docker.sock"
			}

    ],
    "env": [
{
				"name": "PUID",
				"label": "PUID",
				"default": "!PUID"
			},
			{
				"name": "PGID",
				"label": "PGID",
				"default": "!PGID"
			}	

    ]

 }])})
